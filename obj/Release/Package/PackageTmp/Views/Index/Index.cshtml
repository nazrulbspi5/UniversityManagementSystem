					$('.weather-img').attr('src', json.current_observation.icon_url);
					$('.weather').text(json.current_observation.weather);
					$('.weather-degree').text(json.current_observation.temperature_string);
					$('.weather-report').removeClass('hidden');
				});

				$('.scroller').bind('click', function (event) {
					var _anchor = $(this);

					$('a.scroller').each(function (index, el) {
						$($(el).attr('href')).removeClass('padding-top-50');
					});

					if ($(_anchor).attr('href') != '#top') {
						$(_anchor.attr('href')).addClass('padding-top-50');
					}

					$('html, body').stop().animate({
						scrollTop: Math.round($(_anchor.attr('href')).offset().top)
					}, 1500);
					event.preventDefault();
				});

				$(window).scroll(function () {
					if ($(window).scrollTop() > 100) {
						$('.page-topper').show('slow').addClass('floating');
					} else {
						$('.page-topper').hide('slow').removeClass('floating');
					}
				});
			});
